# .env.example
# Essential environment variables - copy to .env and fill in your values.

# --- Required ---
GOOGLE_API_KEY='YOUR_GOOGLE_API_KEY_HERE' # Get the key from aistudio.google.com
GCP_PROJECT_ID='YOUR_GCP_PROJECT_ID'     # Your Google Cloud Project ID
REGION="us-central1"
REPO_ID="adk-agent-images"
SERVICE_NAME="adk-multi-agent-demo"
IMAGE_TAG="latest"

# --- Optional (Defaults) ---
BQ_DEFAULT_LOCATION="US"               # Default BQ dataset location (e.g., US, EU)
VM_DEFAULT_ZONE="us-central1-c"        # Default GCP zone for VMs
VM_DEFAULT_INSTANCE_NAME="instance-via-adk" # Default VM name if not specified
VM_DEFAULT_MACHINE_TYPE="e2-medium"    # Default VM machine type
VM_DEFAULT_SOURCE_IMAGE="projects/debian-cloud/global/images/debian-12-bookworm-v20250311" # Default VM image
VM_DEFAULT_DISK_SIZE_GB="10"           # Default VM disk size (GB)
VM_DEFAULT_DISK_TYPE="pd-balanced"     # Default VM disk type (pd-standard, pd-balanced, pd-ssd)
VM_DEFAULT_SUBNETWORK="subnet-central1" # Default VM subnetwork (ensure it exists in the zone's region)
VM_DEFAULT_SERVICE_ACCOUNT="YOUR_DEFAULT-compute@developer.gserviceaccount.com"          # Default VM service account email (optional, e.g., {PROJECT_NUMBER}-compute@...)